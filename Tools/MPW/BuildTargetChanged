Set echo 0# Will look for exported variable noMenus to disable menu changingIf "{buildTarget}" == Goober	If !{noMenus}		AddMenu {WTVMenu}	' Goober ROM!'			'Set buildTarget Goober; Execute BuildTargetChanged'		AddMenu {WTVMenu}	' TrialSys1 Box ROM'	'Set buildTarget TrialSys1Box; Execute BuildTargetChanged'		AddMenu {WTVMenu}	' IDEV Box ROM'			'Set buildTarget IDEVBox; Execute BuildTargetChanged'	End	Set	TextAddress		"9FC00000"	Set	DataAddress		"80000500"	Set ROMSize			"00040000"	Set ROMTop			"9FC40000"		Set ExtraCPPDefinitions "-d GOOBER -d GOOBER_HACK -d TRIALSYS1 -d TRIAL_SYS_HACK -d CPU_R4640 -d NO_MALLOC -d WEBTV -d DEBUG -d PRERELEASE -d EMBEDDED -d HARDWARE -D __MIPSEB__ -d _MIPS_SZINT=32 -d _MIPS_SZLONG=32 								-d _MIPS_SZPTR=32 -d NO_TTYTYPE -d ROMBASE=0x{TextAddress} -d ROMSIZE=0x{ROMSize}"	Set ExtraCDefinitions "-DGOOBER -DGOOBER_HACK -DTRIALSYS1 -DTRIAL_SYS_HACK -DCPU_R4640 -DNO_MALLOC -DWEBTV -DDEBUG -DPRERELEASE -DEMBEDDED -DHARDWARE -D__MIPSEB__ -D_MIPS_SZINT=32 -D_MIPS_SZLONG=32 								-D_MIPS_SZPTR=32 -DNO_TTYTYPE -DROMBASE=0x{TextAddress} -DROMSIZE=0x{ROMSize}"EndIf "{buildTarget}" == TrialSys1Box	If !{noMenus}		AddMenu {WTVMenu}	' Goober ROM'			'Set buildTarget Goober; Execute BuildTargetChanged'		AddMenu {WTVMenu}	' TrialSys1 Box ROM!'	'Set buildTarget TrialSys1Box; Execute BuildTargetChanged'		AddMenu {WTVMenu}	' IDEV Box ROM'			'Set buildTarget IDEVBox; Execute BuildTargetChanged'	End	Set	TextAddress		"9FC00000"	Set	DataAddress		"80000500"	Set ROMSize			"00400000"	Set ROMTop			"A0000000"		Set ExtraCPPDefinitions "-d TRIALSYS1 -d TRIALSYS1_HACK -d CPU_R4640 -d NO_MALLOC -d WEBTV -d DEBUG -d PRERELEASE -d EMBEDDED -d HARDWARE -D __MIPSEB__ -d _MIPS_SZINT=32 -d _MIPS_SZLONG=32 								-d _MIPS_SZPTR=32 -d NO_TTYTYPE -d ROMBASE=0x{TextAddress} -d ROMSIZE=0x{ROMSize}"	Set ExtraCDefinitions "-DTRIALSYS1 -DTRIALSYS1_HACK -DCPU_R4640 -DNO_MALLOC -DWEBTV -DDEBUG -DPRERELEASE -DEMBEDDED -DHARDWARE -D__MIPSEB__ -D_MIPS_SZINT=32 -D_MIPS_SZLONG=32 								-D_MIPS_SZPTR=32 -DNO_TTYTYPE -DROMBASE=0x{TextAddress} -DROMSIZE=0x{ROMSize}"EndIf "{buildTarget}" == IDEVBox	If !{noMenus}		AddMenu {WTVMenu}	' Goober ROM'			'Set buildTarget Goober; Execute BuildTargetChanged'		AddMenu {WTVMenu}	' TrialSys1 Box ROM'	'Set buildTarget TrialSys1Box; Execute BuildTargetChanged'		AddMenu {WTVMenu}	' IDEV Box ROM!'		'Set buildTarget IDEVBox; Execute BuildTargetChanged'	End	Set	TextAddress		"80200000"	Set	DataAddress		"80000800"	Set ROMSize			"00180000"	Set ROMTop			"80380000"		Set ExtraCPPDefinitions "-d NO_MALLOC -d WEBTV -d DEBUG -d PRERELEASE -d EMBEDDED -d HARDWARE -D __MIPSEB__ -d _MIPS_SZINT=32 -d _MIPS_SZLONG=32 								-d _MIPS_SZPTR=32 -d NO_TTYTYPE -d _381_BOARD_ -d ROMBASE=0x{TextAddress} -d ROMSIZE=0x{ROMSize}"	Set ExtraCDefinitions "-DNO_MALLOC -DWEBTV -DDEBUG -DPRERELEASE -DEMBEDDED -DHARDWARE -D__MIPSEB__ -D_MIPS_SZINT=32 -D_MIPS_SZLONG=32 								-D_MIPS_SZPTR=32 -DNO_TTYTYPE -D_381_BOARD_ -DROMBASE=0x{TextAddress} -DROMSIZE=0x{ROMSize}"End	Set buildOptions "-embedded"	Set CPPCompiler gPlus	Set CCompiler gC		Set CIncludes ""	Set ExtraCIncludes "{{CIncludes}}"	Set ExtraCOptions ""	Set ExtraCCommonOptions ""																Export 	buildTarget buildOptions						CPPCompiler CCompiler 							ExtraCPPDefinitions ExtraCDefinitions			ExtraCOptions 									ExtraCCommonOptions 							ExtraCIncludes